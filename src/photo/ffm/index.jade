extends ../../_layouts/base

include ../../_modules/header/header
include ../../_modules/footer/footer

block config
  //- Correct base url pathing for this page
  - var baseUrl = '../../'
  - var siteTitle = 'Frankfurt 2017'
  - var siteDescription = 'Photos from my Frankfurt trip in May 2017'

block content
